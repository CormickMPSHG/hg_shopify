<script>// <![CDATA[
$(document).ready(function() {
			//discount switcher
			/*var ran_number = !!Math.round(Math.random() * 1); //creates random boolean*/
			var ran_number = 'true';
			if (!ran_number) {
              $("#mc_embed_signup_fancybox h2").html("Sign up and <br />save 20% on <br />your first order!");
			} else {
				$("#mc_embed_signup_fancybox h2").html("Sign up and <br />receive a $25 <br />gift certificate!");	
			}
			$("#discountSwitcher").val(ran_number);
			// variable to hold request
			var request;
			// bind to the submit event of our form
			$("#mc-embedded-subscribe-form_fancybox").submit(function(event){
				// abort any pending request
				if (request) {
					request.abort();
				}
				// setup some local variables
				var $form = $(this);
				// let's select and cache all the fields
				var $inputs = $form.find("input, select, button, textarea");
				// serialize the data in the form
				var serializedData = $form.serialize();
			
				// let's disable the inputs for the duration of the ajax request
				$inputs.prop("disabled", true);
			
				$("#mce-error-response_fancybox").hide();
				$('#mce-success-response_fancybox').html('Adding email address...').show();
				$.colorbox.resize();
				
				// fire off the request
				request = $.ajax({
					url: "http://madisonplus.com/heygorgeous/list-subscribe.php",
					type: "get",
					data: serializedData,
					success: function(data){
						if(data.error == true) {
							var errormsg = data.error_msg.join()
							errormsg = errormsg.replace("Click here to update your profile.","");
							$("#mce-error-response_fancybox").html(errormsg).show();
							$('#mce-success-response_fancybox').hide();
							$.colorbox.resize();
						} else {
							var coupon = "";
							if(data.coupon) {
								coupon = data.coupon;
							}
							//$("#mce-success-response").html(data.success_msg.join());
							window.location.replace('http://www.heygorgeous.com/pages/email-signup-confirmation?c='+coupon);
						}
					},
					error:function(data){
						console.log(JSON.stringify(data));
						$("#mce-error-response_fancybox").html('There was an error while submitting the form.  Please try again later.').show();
						$('#mce-success-response_fancybox').hide();
						$.colorbox.resize();
					}
				});
			
				// callback handler that will be called regardless
				// if the request failed or succeeded
				request.always(function () {
					// reenable the inputs
					$inputs.prop("disabled", false);
				});
			
				// prevent default posting of form
				event.preventDefault();
			});
			
			//open colorbox
			//wait 12 seconds
			setTimeout(function () {
				//open colorbox first visit		
				if(document.cookie.indexOf('visited=true') == -1) {			
					var fifteenDays = 1000*60*60*24*15;			
					var expires = new Date((new Date()).valueOf() + fifteenDays);			
					document.cookie = "visited=true;expires=" + expires.toUTCString();			
					$.colorbox({				
						'inline':true,							
						'href':"#mc_embed_signup_fancybox",
						'maxWidth':"95%",
						'onComplete':function() {
							ga('send', 'pageview', {
							  'page': '/pop-up-email-subscribe-form',
							  'title': 'Pop Up Email Subscribe Form'
							});
						},
						'onClosed':function() {
							ga('send', 'event', 'pop-up', 'click', 'no-thanks');
						}
					});		
				}
			}, 12000);
			
			//close colorbox link
			$("#closeform").click(function() {
				$.colorbox.close();
			});
		});
// ]]></script>
